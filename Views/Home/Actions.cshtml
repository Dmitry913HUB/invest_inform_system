
@{
    ViewBag.Title = "Actions";
}



<div class="mainTable">
    <h3>Валюта</h3>
    <table>
        <tr>
            <td>Id</td>
            <td>RUB</td>
            <td>USD</td>
            <td>EUR</td>
            <td></td>
        </tr>
        @foreach (var b in ViewBag.Score)
        {
    <tr>
        <td>@b.Id</td>
        <td>@b.RUB</td>
        <td>@b.USD</td>
        <td>@b.EUR</td>
        <td>
            <button><a href="/Home/Replenishment/@b.Id">Пополнить</a></button>
        </td>
        <td>
            <button><a href="/Home/Transfer/@b.Id">Перевести</a></button>
        </td>
    </tr>
        }
    </table>
</div>

<div class="mainTable">
    <h3>Акции</h3>
    <table>
        <tr>
            <td>Id</td>
            <td>Название</td>
            <td>Цена</td>
            <td>Валюта</td>
            <td></td>
        </tr>
        @foreach (var b in ViewBag.Shares)
        {
    <tr>
        <td>@b.Id</td>
        <td>@b.Name</td>
        <td>@b.Cost</td>
        <td>@b.TypeVal</td>
        <td>
            <button><a href="/Home/IssueBuy/@b.Id">Купить</a></button>
        </td>
    </tr>
        }
    </table>
</div>




